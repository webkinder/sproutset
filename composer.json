{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "webkinder/sproutset",
  "description": "A Composer package for handling responsive images in Roots Bedrock + Sage + Blade projects.",
  "keywords": [
    "responsive images",
    "bedrock",
    "sage",
    "blade"
  ],
  "homepage": "https://github.com/webkinder/sproutset",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Marco Luzi",
      "email": "marco@webkinder.ch"
    },
    {
      "name": "Raphael Portmann",
      "email": "raphael@webkinder.ch"
    }
  ],
  "require": {
    "php": "^8.2",
    "roots/acorn": "^5.0",
    "roots/wordpress-no-content": "^5.9 || ^6.0",
    "spatie/image": "^3.8",
    "spatie/image-optimizer": "^1.8"
  },
  "require-dev": {
    "driftingly/rector-laravel": "2.1.4",
    "laravel/pint": "1.26.0",
    "rector/rector": " 2.2.9",
    "wp-cli/i18n-command": "2.6.5"
  },
  "autoload": {
    "psr-4": {
      "Webkinder\\SproutsetPackage\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "acorn": {
      "providers": [
        "Webkinder\\SproutsetPackage\\SproutsetServiceProvider"
      ]
    }
  }
}
